import java.util.Scanner;

class MultiplicationTable{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        
        System.out.print("Please enter first number: ");
        int num1 = in.nextInt();
        
        for(int i=1;i<11;i++){
            System.out.print("\n"+num1+"*"+i+"="+(num1*i));
        }
    }
}
